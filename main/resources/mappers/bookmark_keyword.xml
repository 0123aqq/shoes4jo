<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="BookMarkKeyword">

	<resultMap type="com.multi.shoes4jo.vo.BookmarkK"
		id="bookmarkKResult">

		<result property="bookmark_id" column="bookmark_id" />
		<result property="member_id" column="member_id" />
		<result property="start_date" column="start_date" />
		<result property="end_date" column="end_date" />
		<result property="time_unit" column="time_unit" />
		<result property="keywords" column="keywords" />
		<result property="device" column="device" />
		<result property="gender" column="gender" />
		<result property="ages" column="ages" />
		<result property="add_date" column="add_date" />

	</resultMap>

	<insert id="insertBookmarkK"
		parameterType="com.multi.shoes4jo.vo.BookmarkC">
		INSERT INTO shoes_4jo.bookmark_keyword (bookmark_id, member_id, start_date, end_date,
		time_unit,
		keywords, device, gender, device, ages,
		add_date)
		VALUES (#{bookmark_id}, #{member_id}, #{start_date}, #{end_date},
		#{time_unit}, #{keywords}, #{device}, #{gender},
		#{device}, #{ages}, #{add_date})
	</insert>

	<delete id="deleteBookmarkK"
		parameterType="com.multi.shoes4jo.vo.BookmarkC">
		DELETE FROM shoes_4jo.bookmark_keyword
		WHERE bookmark_id = #{bookmark_id}
	</delete>

</mapper>

